<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:client.View">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/View/Themes/Base.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="WindowBorderStyle" TargetType="Border">
        <Setter Property="Background" Value="#0d231c"></Setter>
        <Setter Property="BorderBrush" Value="#609a87"></Setter>
    </Style>
    <Style x:Key="PanelBorderStyle">
        <Setter Property="Control.Background" Value="#00da93"></Setter>
    </Style>
    <Style x:Key="ButtonBorderStyle" TargetType="Border">
        <Setter Property="Background" Value="#48826f"></Setter>
    </Style>
</ResourceDictionary>